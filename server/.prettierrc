{
  "printWidth": 80,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrder": [
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "^(express)(/.*)?$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/controllers(/.*)?$",
    "",
    "^@/(middlewares|config|data)(/.*)?$",
    "",
    "^@/(utils|constants|schemas)(/.*)?$",
    "",
    "^[.]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
